{
  "name": "qa-site",
  "private": true,
  "version": "0.1.0",
  "homepage": "https://fai9al2001.github.io/QandA/",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
  "build:docs": "tsc -b && vite build --outDir docs",
    "build:pages": "cross-env VITE_BASE=%VITE_BASE% npm run build",
    "copy404": "copy dist\\index.html dist\\404.html",
    "predeploy": "npm run build && npm run copy404",
    "deploy": "gh-pages -d dist -b gh-pages",
    "preview": "vite preview",
    "lint": "eslint . --ext .ts,.tsx"
  },
  "dependencies": {
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-router-dom": "^6.23.1",
    "classnames": "^2.5.1"
  },
  "devDependencies": {
    "@types/node": "^20.12.7",
    "@types/react": "^18.2.74",
    "@types/react-dom": "^18.2.24",
    "autoprefixer": "^10.4.19",
  "eslint": "^8.57.0",
  "@typescript-eslint/parser": "^7.18.0",
  "@typescript-eslint/eslint-plugin": "^7.18.0",
    "postcss": "^8.4.38",
    "tailwindcss": "^3.4.7",
  "typescript": "^5.4.5",
  "vite": "^5.2.0",
  "@vitejs/plugin-react": "^4.2.0",
  "eslint-plugin-react-refresh": "^0.4.5",
  "gh-pages": "^6.1.1",
  "cross-env": "^7.0.3"
  }
}
